pipeline {
    agent any

    tools {
        maven 'Maven3'
    }

    stages {
        stage('Build') {
            steps {
                echo "Building the project"
                sh 'mvn clean install -f MyWebApp/pom.xml'
            }
        }
    
    }
    
   }

        

        
            
        

        


      
        

      
      
        

       
    

